	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">

<bean id="book" class="org.springcore.demo.model.Book">
<property name="isbn" value="50"></property>
<property name="name" value="My Career"></property>
<property name="volumes">
<array>
<value> at wipro</value>
<value> when i quit the job</value>
<value> when i started my youtube</value>

</array>

</property>

</bean>



<bean id="book2" class="org.springcore.demo.model.Book">
<property name="isbn" value="55"></property>
<property name="name" value="My Career"></property>
<property name="volumes">
<array>
<value> at wipro</value>
<value> when i quit the job</value>
<value> when i started my youtube</value>

</array>

</property>

</bean>




<bean id="library" class="org.springcore.demo.model.Library">
<property name="libraryName" value="City Central Library"></property>

<property name="book">

 <array>
 <ref bean="book"/> 
 <ref bean="book2"/> 
 </array>
 

</property>

</bean>



</beans>

